{
    "name": "Moodle JSON",
    "version": "1.0",
    "manifest_version" : 2,
    "description": "Downloads names and submissions from moodle and gives JSON",
    "background" : {
      "scripts" : ["background.js"]
    },
    "browser_action": {
      "default_icon": "logo.png"
    },
    "permissions": ["activeTab"]
  }